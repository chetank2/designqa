{
  "name": "@designqa/shared-types",
  "version": "2.0.1",
  "description": "Shared TypeScript type definitions for DesignQA applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./extractor": {
      "import": "./dist/extractor.js",
      "types": "./dist/extractor.d.ts"
    },
    "./api": {
      "import": "./dist/api.js",
      "types": "./dist/api.d.ts"
    }
  }
}