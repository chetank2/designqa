{
  "name": "@designqa/desktop-win",
  "version": "2.0.1",
  "description": "DesignQA Desktop App for Windows",
  "main": "dist/main/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "tsc --watch --project tsconfig.main.json",
    "dev:renderer": "vite",
    "build": "npm run build:workspace-packages && npm run verify:workspace-packages && npm run build:frontend && npm run copy:backend && npm run install:backend-deps && npm run install:deps && npm run resolve:deps && npm run build:main && npm run copy:renderer && npm run check:ts-syntax",
    "build:frontend": "cd ../saas-frontend && pnpm build:electron",
    "copy:renderer": "mkdir -p dist/renderer && cp -r ../saas-frontend/dist/* dist/renderer/",
    "install:backend-deps": "node scripts/install-backend-deps.js",
    "install:deps": "pnpm install --filter @designqa/desktop-win",
    "resolve:deps": "node scripts/resolve-deps.js",
    "build:main": "tsc --project tsconfig.main.json",
    "build:renderer": "vite build",
    "build:workspace-packages": "cd ../../packages/mcp-client && pnpm run build && cd ../compare-engine && pnpm run build",
    "verify:workspace-packages": "node scripts/verify-build.js",
    "check:ts-syntax": "node scripts/check-ts-syntax.js",
    "copy:backend": "node scripts/copy-backend.js",
    "package": "electron-builder --win",
    "package:all": "electron-builder --win --mac"
  },
  "dependencies": {
    "@designqa/mcp-client": "workspace:*",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@myapp/compare-engine": "workspace:*",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.87.1",
    "@tanstack/react-query": "^5.90.16",
    "axios": "^1.13.2",
    "canvas": "^3.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "framer-motion": "^12.23.25",
    "lucide-react": "^0.542.0",
    "multer": "^2.0.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.68.0",
    "react-router-dom": "^7.10.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.13.3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  },
  "build": {
    "appId": "com.designqa.desktop",
    "productName": "DesignQA",
    "asar": true,
    "npmRebuild": false,
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "build/icon.ico",
      "forceCodeSigning": false
    },
    "files": [
      "dist/**/*",
      "package.json",
      "node_modules/**/*",
      "!**/node_modules/**/{README.md,README,readme.md,readme}",
      "!**/node_modules/**/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/**/*.d.ts",
      "!**/node_modules/**/*.map",
      "!**/node_modules/**/.{eslintrc,prettierrc,prettierignore,eslintignore}",
      "!**/node_modules/**/{.nyc_output,coverage,.nyc_output}",
      "saas-backend/src/**/*",
      "saas-backend/package.json",
      "saas-backend/config.json",
      "saas-backend/node_modules/**/*",
      "!saas-backend/dist/**/*",
      "!saas-backend/tests/**/*",
      "!**/*.ts",
      "!**/*.tsx",
      "!**/src/**/*.ts",
      "!**/src/**/*.tsx",
      "!**/packages/**/src/**",
      "!**/packages/**/*.ts",
      "!**/packages/**/*.tsx"
    ],
    "asarUnpack": [
      "saas-backend/src/**/*",
      "saas-backend/node_modules/**/*"
    ],
    "directories": {
      "output": "build"
    }
  }
}
