{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./dist/main",
    "rootDir": "./src/main",
    "module": "ESNext",
    "target": "ES2022",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true
  },
  "include": ["src/main/**/*"],
  "exclude": [
    "node_modules",
    "dist",
    "**/saas-backend/**",
    "../../saas-backend/**",
    "../saas-backend/**",
    "../../../saas-backend/**",
    "**/*.js"
  ],
  "compilerOptions": {
    "outDir": "./dist/main",
    "rootDir": "./src/main",
    "module": "ESNext",
    "target": "ES2022",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": false
  }
}
